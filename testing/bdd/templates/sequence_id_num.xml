<?xml version="1.0" ?>
<tt:tt
{% if sequence_id %}
  {% if sequence_id == "*?Empty?*" %}
        ebuttp:sequenceIdentifier=""
  {% else %}
        ebuttp:sequenceIdentifier="{{ sequence_id }}"
  {% endif %}
{% endif %}
{% if sequence_num %}
  {% if sequence_num == "*?Empty?*" %}
        ebuttp:sequenceNumber=""
  {% else %}
        ebuttp:sequenceNumber="{{ sequence_num }}"
  {% endif %}
{% endif %}
        ttp:clockMode="local"
        ttp:timeBase="clock"
        xml:lang="en-GB"
        xmlns:ebuttm="urn:ebu:tt:metadata"
        xmlns:ebuttp="urn:ebu:tt:parameters"
        xmlns:tt="http://www.w3.org/ns/ttml"
        xmlns:ttp="http://www.w3.org/ns/ttml#parameter"
        xmlns:xml="http://www.w3.org/XML/1998/namespace">
  <tt:head>
    <tt:metadata>
      <ebuttm:documentMetadata/>
    </tt:metadata>
    <tt:styling>
      <tt:style xml:id="ID001"/>
    </tt:styling>
    <tt:layout/>
  </tt:head>
  <tt:body tt:begin="500ms" tt:dur="00:00:05">
    <tt:div>
      <tt:p tt:begin="500ms" tt:end="3420ms" xml:id="ID005">
        <tt:span>Some example text...</tt:span>
        <tt:br/>
        <tt:span>And another line</tt:span>
      </tt:p>
    </tt:div>
  </tt:body>
</tt:tt>
