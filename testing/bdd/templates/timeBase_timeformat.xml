<?xml version="1.0" ?>

<!--  Note: contains SMPTE time base. SMPTE was removed from the specification in version 1.0. -->

<tt:tt
        ebuttp:sequenceIdentifier="testSequence001"
        ebuttp:sequenceNumber="1"
        ttp:timeBase="{{ time_base }}"
{% if time_base == 'smpte' %}
        ttp:frameRate="25"
        ttp:dropMode="nonDrop"
        ttp:markerMode="continuous"
        ttp:frameRateMultiplier="1 1"
{% endif %}
{% if time_base == 'clock' %}
        ttp:clockMode='local'
{% endif %}
        xml:lang="en-GB"
        xmlns:ebuttm="urn:ebu:tt:metadata"
        xmlns:ebuttp="urn:ebu:tt:parameters"
        xmlns:tt="http://www.w3.org/ns/ttml"
        xmlns:ttp="http://www.w3.org/ns/ttml#parameter"
        xmlns:xml="http://www.w3.org/XML/1998/namespace">
  <tt:head>
    <tt:metadata>
      <ebuttm:documentMetadata/>
    </tt:metadata>
    <tt:styling>
      <tt:style xml:id="ID001"/>
    </tt:styling>
  </tt:head>
  <tt:body
{% if body_begin %}
  {% if body_begin == "*?Empty?*" %}
      begin=""
  {% else %}
      begin="{{ body_begin }}"
  {% endif %}
{% endif %}
{% if body_end %}
  {% if body_end == "*?Empty?*" %}
      end=""
  {% else %}
      end="{{ body_end }}"
  {% endif%}
{% endif %}
{% if body_dur %}
  {% if body_dur == "*?Empty?*" %}
      dur=""
  {% else %}
      dur="{{ body_dur }}"
  {% endif %}
{% endif %}
    >
    <tt:div
{% if div_begin %}
  {% if div_begin == "*?Empty?*" %}
            begin=""
  {% else %}
            begin="{{ div_begin }}"
  {% endif %}
{% endif %}
{% if div_end %}
  {% if div_end == "*?Empty?*" %}
            end=""
  {% else %}
            end="{{ div_end }}"
  {% endif %}
{% endif %}
      >
      <tt:p
{% if p_begin %}
  {% if p_begin == "*?Empty?*" %}
            begin=""
  {% else %}
            begin="{{ p_begin }}"
  {% endif %}
{% endif %}
{% if p_end %}
  {% if p_end == "*?Empty?*" %}
            end=""
  {% else %}
            end="{{ p_end }}"
  {% endif %}
{% endif %}
            xml:id="ID005">
            <tt:span
{% if span_begin %}
  {% if span_begin == "*?Empty?*" %}
                begin=""
  {% else %}
                begin="{{ span_begin }}"
  {% endif %}
{% endif %}
{% if span_end %}
  {% if span_end == "*?Empty?*" %}
                end=""
  {% else %}
                end="{{ span_end }}"
  {% endif %}
{% endif %}
            >
              Some example text...
            </tt:span>
        <tt:br/>
        <tt:span>And another line</tt:span>
      </tt:p>
    </tt:div>
  </tt:body>
</tt:tt>
